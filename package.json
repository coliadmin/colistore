{
  "name": "coliburgers",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start-redis": "docker compose -f docker/local/compose.yaml up -d",
    "stop-redis": "docker compose -f docker/local/compose.yaml down",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "d": "docker compose -f docker/local/compose.yaml up -d & next dev",
    "build:dev": "docker compose -f docker/development/compose.yaml build",
    "start:dev": "docker compose -f docker/development/compose.yaml up -d",
    "stop:dev": "docker compose -f docker/development/compose.yaml down",
    "run:dev": "pnpm build:dev & pnpm start:dev",
    "build:stg": "docker compose -f docker/staging/compose.yaml build",
    "start:stg": "docker compose -f docker/staging/compose.yaml up -d",
    "stop:stg": "docker compose -f docker/staging/compose.yaml down",
    "run:stg": "build:stg & start:stg",
    "build:prd": "docker compose -f docker/production/compose.yaml build",
    "start:prd": "docker compose -f docker/production/compose.yaml up -d",
    "stop:prd": "docker compose -f docker/production/compose.yaml down",
    "run:prd": "build:prd & start:prd"
  },
  "engines": {
    "node": ">=22.13 <23",
    "pnpm": ">=9"
  },
  "dependencies": {
